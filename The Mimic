    print('Hello!')
end)


-- Create the third tab with a different image ID
local tab3 = DrRayLibrary.newTab("B2C3", "ImageIdLogoHere")

-- Add elements to the third tab

tab3.newLabel("GrinDemon")
tab3.newButton("Esp GrinDemon", "Monster/Torch part", function()
     -- loadstring
     local ESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()
 
     -- config
     ESP.Players = false
     ESP.Boxes = true
     ESP.Names = true
     ESP:Toggle(true)
 
     -- object
    ESP:AddObjectListener(Workspace.Section1.Maze, {
        Name = "GrinDemon", --Object name inside of the path, for example: Workspace.ThisFolder.Item_1
        CustomName = "Monster", -- Name you want to be displayed
        Color = Color3.fromRGB(255, 0, 0), -- Color
        IsEnabled = "whatever" -- Any name, has to be the same as the last line: ESP.TheNameYouWant
    })
    ESP.whatever = true
end)

tab3.newLabel("Dodomeki")
