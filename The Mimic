-- Create the another tab with a different image ID
local tab9 = DrRayLibrary.newTab("Misc", "ImageIdLogoHere")

tab9.newLabel("In-Game Helper")
tab9.newButton("Instance interact", "instance E", function()
     game:GetService("ProximityPromptService").PromptButtonHoldBegan:Connect(function(prompt)
  fireproximityprompt(prompt)
  end)
end)

tab9.newButton("Fullbright", "too bright!", function()
     local Light = game:GetService("Lighting")

     function dofullbright()
     Light.Ambient = Color3.new(1, 1, 1)
     Light.ColorShift_Bottom = Color3.new(1, 1, 1)
     Light.ColorShift_Top = Color3.new(1, 1, 1)
     end

     dofullbright()

     Light.LightingChanged:Connect(dofullbright)
end)

tab9.newButton("Infinite yield", "tap to execute!", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)



