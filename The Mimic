-- Load the DrRay library from the GitHub repository Library
local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

-- Create a new window and set its title and theme
local window = DrRayLibrary:Load("The Mimic", "Default")

-- Create the first tab with an image ID
local tab1 = DrRayLibrary.newTab("B2C1", "ImageIdHere")

-- Add elements to the first tab
tab1.newLabel("WIP!")
tab1.newButton("Soon!", "Esp", function()
    print('Soon')
end)

-- Create the second tab with a different image ID
local tab2 = DrRayLibrary.newTab("B2C2", "ImageIdLogoHere")

-- Add elements to the second tab
tab2.newLabel("WIP!")
